<?xml version="1.0" encoding="utf-8" ?>
<Scintilla xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<keyword-classes>
		<keyword-class name="json-keywords">
			false true null
		</keyword-class>
		<keyword-class name="jsonld-keywords">
			@id @context @type @value @language @container
			@list @set @reverse @index @base @vocab @graph
		</keyword-class>
	</keyword-classes>
	<languages>
		<language name="json">
			<lexer name="json" style-bits="7" />
			<character-class><![CDATA[qwertyuiopasdfghjklzxcvbnmQWERTYUIOPASDFGHJKLZXCVBNM0123456789-_$]]></character-class>
			<file-extensions>json,eshintrc,jshintrc,flump</file-extensions>
			<line-comment><![CDATA[//]]></line-comment>
			<comment-start><![CDATA[/*]]></comment-start>
			<comment-end><![CDATA[*/]]></comment-end>
			<use-keywords>
				<keyword key="0" class="json-keywords" />
				<keyword key="1" class="jsonld-keywords" />
			</use-keywords>
			<!-- COLORING_START -->
			<editor-style caret-fore="0x000000" caretline-back="0xececec" selection-fore="0xffffff" selection-back="0x3399ff" colorize-marker-back="true" margin-fore="0xffffff" margin-back="0xffffff" marker-back="0xbcbcbc" />
			<use-styles>
				<style name="default" fore="0x000000" back="0xffffff" size="9" font="Consolas,Courier New,monospace" />
				<style name="number" fore="0x000099" />
				<style name="string" fore="0xa31515" />
				<style name="stringeol" fore="0xff0000" />
				<style name="propertyname" fore="0x000033" />
				<style name="escapesequence" />
				<style name="linecomment" fore="0x008000" />
				<style name="blockcomment" fore="0x008000" />
				<style name="operator" />
				<style name="uri" fore="0x0060ff" />
				<style name="compactiri" fore="0x0060ff" />
				<style name="keyword" fore="0x000099" />
				<style name="ldkeyword" fore="0x000099" />
				<style name="error" fore="0xff0000" />
				<style name="gdefault" fore="0xefece9" />
				<style name="linenumber" fore="0x666666" />
				<style name="bracelight" fore="0x0000cc" back="0xcdcdff" bold="true" />
				<style name="bracebad" bold="true" />
				<style name="controlchar" fore="0xffffff" />
				<style name="indentguide" fore="0xc0c0c0" />
				<style name="lastpredefined" fore="0x666666" />
			</use-styles>
			<!-- COLORING_END -->
		</language>
	</languages>
</Scintilla>
